<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="eai.devass.gsr.appli.modele.metier.reglement">
	<class name="OrderesVirements" table="GSR_ORDREVIREMENT">
		<id name="id" type="long">
			<column name="ID" precision="22" scale="0" />
			<generator class="increment" />
		</id>
		<property name="annee" column="annee" />
		<property name="commission" column="commission" />
		<property name="commissionH" column="commission_h" />
		<property name="dateEdition" column="date_edition" />
		<property name="edit" column="edit" />
		<property name="montant" column="montant" />
		<property name="nombre" column="nombre" />
		<property name="nomFichier" column="nom_fichier" />
		<property name="numeroVirement" column="numero_virement" />
		<property name="trimestre" column="trimestre" />
		<property name="typeRente" column="type_rente" />
		<property name="typeVirement" column="type_virement" />
		<property name="dateCreation" column="date_creation" />

	</class>
</hibernate-mapping>