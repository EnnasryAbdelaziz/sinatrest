<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="eai.devass.sinistreat.appli.modele.parametrage.EmailIntermediaire"
		table="EMAIL_INTERMEDIAIRE">
		<id name="code">
			<column name="CODE" />
		</id>
		<property name="type">
			<column name="TYPE" />
		</property>
		<property name="email">
			<column name="EMAIL" />
		</property>

	</class>
</hibernate-mapping>