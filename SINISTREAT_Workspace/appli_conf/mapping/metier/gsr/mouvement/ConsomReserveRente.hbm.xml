<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="eai.devass.gsr.appli.modele.metier.mouvements">
	<class name="ConsomReserveRente" table="GSR_CONSOM_RESERVE_RENTE">
		<id name="id" type="long">
			<column name="ID" precision="22" scale="0" />
			<generator class="increment" />
		</id>
		<property name="numeroQuittance" column="numero_quittance" />
		<property name="reserveApres" column="reserve_apres" />
		<property name="reserveAvant" column="reserve_avant" />
		<property name="dateCreation" column="date_creation" />

	</class>
</hibernate-mapping>